<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ximalaya.damus.example.ad.mapper.PositionMapper">
    <select id="getAllPositions" resultType="com.ximalaya.damus.example.ad.bo.Position">
        SELECT
            id,
            title AS title
        FROM
            position
    </select>

</mapper>